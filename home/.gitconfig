    name = jpic
[core]
    autocrlf = input
    excludesfile = /home/jpic/.gitignore_global
    editor = /usr/bin/vim
	quotepath = false
[http]
    sslVerify = false

[color]
     ui = auto
[color "branch"]
     current = yellow reverse
     local = yellow
     remote = green
[color "diff"]
     meta = yellow bold
     frag = magenta bold
     old = red bold
     new = green bold
[color "status"]
     added = yellow
     changed = green
     untracked = cyan
[diff]
	tool = vimdiff
[gitreview]
	scheme = https
	port = 443
	username = jpic
